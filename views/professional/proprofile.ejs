<!DOCTYPE html>
<html lang="en">

<%- include("../header.ejs") %>
<head>
    <link href="css/sb-admin-2.min.css" rel="stylesheet">
    
</head>

<body id="page-top">

    <!-- Page Wrapper -->
    <div id="wrapper">

        <!-- Sidebar -->
        <ul class="navbar-nav bg-gradient-primary sidebar sidebar-dark accordion" id="accordionSidebar">

            <!-- Sidebar - Brand -->
            <a class="sidebar-brand d-flex align-items-center justify-content-center" href="/prodashboard">

                <div class="sidebar-brand-text mx-3">Find A Professional</div>
            </a>

            <!-- Divider -->
            <hr class="sidebar-divider my-0">

            <!-- Nav Item - Dashboard -->
            <li class="nav-item active">
                <a class="nav-link" href="/prodashboard">
                    <i class="fas fa-fw fa-tachometer-alt"></i>
                    <span>Dashboard</span></a>
            </li>

            <!-- Divider -->
            <hr class="sidebar-divider">

            <li class="nav-item active">
                <a class="nav-link" href="/proprofile">
                    <i class="fa fa-user"></i>
                    <span>Professional Profile</span></a>
            </li>
            
            
            <hr class="sidebar-divider d-none d-md-block">
            <!-- Nav Item - Tables -->
            <li class="nav-item">
                <a class="nav-link" href="/progress">
                    <i class="fas fa-fw fa-table"></i>
                    <span>Booking In Progress</span></a>
            </li>

            <!-- Divider -->
            <hr class="sidebar-divider d-none d-md-block">

            <li class="nav-item">
                <a class="nav-link" href="/prohistory">
                    <i class="fas fa-fw fa-table"></i>
                    <span>History Services</span></a>
            </li>

            <!-- Divider -->
            <hr class="sidebar-divider d-none d-md-block">

            <!-- Sidebar Toggler (Sidebar) -->
            <div class="text-center d-none d-md-inline">
                <button class="rounded-circle border-0" id="sidebarToggle"></button>
            </div>



        </ul>
        <!-- End of Sidebar -->

        <!-- Content Wrapper -->
        <div id="content-wrapper" class="d-flex flex-column">

            <!-- Main Content -->
            <div id="content">

                <!-- Topbar -->
                <nav class="navbar navbar-expand navbar-light bg-white topbar mb-4 static-top shadow">
                    <!-- Topbar Navbar -->
                    
                    <ul class="navbar-nav ml-auto">
                        <!-- Nav Item - User Information -->
                        <button class="w3-button w3-hover-light-grey"><a href="/logout">Log out</a></button>
                    </ul>
                </nav>
                <!-- End of Topbar -->
                <div class="row">

                    <div class="container">
                        <div class="card shadow mb-4" style="width: 100%; margin:auto; display:block;">

                            <div class="card-header py-3" style="text-align:center;">
                                <h3>My Profile</h3>
                            </div>

                            <div class="card-deck m-auto ">
                                <div class="card">
                                    <div class="card-body">
                                        <img src="<%= pro.image %>" class="img-thumbnail " id="output" alt="Image Preview">
                                        <div class="jarakInput">
                                            <label for="name"><a>Name : </a></label>
                                            <input type="text" name="nname" value="<%= pro.username %>"
                                                class="form-control form-control-user" readonly>
                                        </div>
                                        <hr class="sidebar-divider">
                                        <div class="jarakInput">
                                            <label for="uname"><a>Email :</a></label>
                                            <input type="email" name="nemail" value=" <%= pro.email %>"
                                                class="form-control form-control-user" readonly>
                                        </div>
                                        <hr class="sidebar-divider">
                                        <div class="jarakInput">
                                            <label for="uname"><a>Password :</a></label>
                                            <input type="text" name="npass" value=" <%= pro.passwordConf %>"
                                                class="form-control form-control-user" readonly>
                                        </div>
                                        <hr>
                                        <a href="/editproprofile" class="btn btn-primary btn-user btn-block" >Update
                                            Profile</a>
                                    </div>
                                </div>
                                <br>
                                <div class="card">
                                    <div class=" card-body">
                                        <div class="jarakInput">
                                            <label for="name"><a>Catergory : </a></label>
                                            <input type="text" name="nname" value="<%= pro.categoryname %>"
                                                class="form-control form-control-user" readonly>
                                        </div>
                                        <hr class="sidebar-divider">
                                        <div class="jarakInput">
                                            <label for="uname"><a>Career :</a></label>
                                            <input type="text" name="nemail" value=" <%= pro.career %>"
                                                class="form-control form-control-user" readonly>
                                        </div>
                                        <hr class="sidebar-divider">
                                        <div class="jarakInput">
                                            <label for="uname"><a>Desciption Career :</a></label>
                                            <input type="text" name="npass" value=" <%= pro.desc %>"
                                                class="form-control form-control-user" style="height:80px;" readonly>
                                        </div>
                                        <hr>
                                        <a href="/editcarrier" class="btn btn-primary btn-user btn-block"> Update Carrier</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
        </div>


                    <!-- Bootstrap core JavaScript-->
                    <script src="vendor/jquery/jquery.min.js"></script>
                    <script src="vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

                    <!-- Core plugin JavaScript-->
                    <script src="vendor/jquery-easing/jquery.easing.min.js"></script>

                    <!-- Custom scripts for all pages-->
                    <script src="js/sb-admin-2.min.js"></script>

                    <!-- Page level plugins -->
                    <script src="vendor/datatables/jquery.dataTables.min.js"></script>
                    <script src="vendor/datatables/dataTables.bootstrap4.min.js"></script>

                    <!-- Page level custom scripts -->
                    <script src="js/demo/datatables-demo.js"></script>

                    <script type="text/javascript" src="js/mdb.min.js"></script>
                    <!-- Custom scripts -->


</body>

</html>